{
  "name": "portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build-storybook": "storybook build",
    "build:ci": "pnpm test -- --ci --collectCoverage && pnpm export",
    "dev": "next dev",
    "export": "next build && next-export-optimize-images",
    "format-code:all": "prettier --write ./src",
    "format-code:staged": "pretty-quick --staged",
    "lint": "eslint src",
    "lint:staged": "lint-staged",
    "pre-commit": "pnpm format-code:staged && pnpm lint:staged",
    "prepare": "husky install",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "test": "jest",
    "test:coverage": "jest --collectCoverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm pre-commit",
      "pre-push": "pnpm test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "jest --bail --findRelatedTests"
    ]
  },
  "dependencies": {
    "@floating-ui/react": "^0.25.0",
    "@netlify/next": "^1.4.4",
    "@netlify/plugin-nextjs": "^4.31.0",
    "@sentry/nextjs": "^7.61.1",
    "classnames": "^2.3.2",
    "downshift": "^8.1.0",
    "next": "^13.4.12",
    "next-export-optimize-images": "^3.1.0",
    "next-images": "^1.7.0",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.2",
    "react-icons": "^4.2.0",
    "react-spring": "^8.0.27",
    "react-toastify": "^8.0.2",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@storybook/addon-essentials": "7.2.1",
    "@storybook/addon-interactions": "7.2.1",
    "@storybook/addon-links": "7.2.1",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/addon-styling": "^1.3.5",
    "@storybook/blocks": "7.2.1",
    "@storybook/nextjs": "7.2.1",
    "@storybook/react": "7.2.1",
    "@storybook/testing-library": "0.2.0",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^13.2.3",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": ">=6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.23",
    "postcss-import": "^15.1.0",
    "prettier": "^3.0.0",
    "pretty-quick": "^3.1.1",
    "storybook": "7.2.1",
    "style-loader": "^2.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": "^16.19.0"
  },
  "overrides": {
    "@testing-library/dom": "^9.0.1"
  }
}
